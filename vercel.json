{
  "version": 2,
  "crons": [
    {
      "path": "/api/cron/finalize-locacoes",
      "schedule": "0 5 * * *"
    }
  ],
  "rewrites": [
    {
      "source": "/api/cron/finalize-locacoes",
      "destination": "/api/cron/finalize-locacoes.mjs"
    },
    {
      "source": "/api/locacoes",
      "destination": "/api/locacoes.mjs"
    },
    {
      "source": "/api/locacoes/(.*)",
      "destination": "/api/locacoes.mjs?path=$1"
    },
    {
      "source": "/api/clientes",
      "destination": "/api/clientes.mjs"
    },
    {
      "source": "/api/clientes/(.*)",
      "destination": "/api/clientes.mjs?path=$1"
    },
    {
      "source": "/api/veiculos",
      "destination": "/api/veiculos.mjs"
    },
    {
      "source": "/api/veiculos/(.*)",
      "destination": "/api/veiculos.mjs?path=$1"
    },
    {
      "source": "/api/dashboard",
      "destination": "/api/dashboard.mjs"
    },
    {
      "source": "/api/movimentacoes",
      "destination": "/api/movimentacoes.mjs"
    },
    {
      "source": "/api/movimentacoes/(.*)",
      "destination": "/api/movimentacoes.mjs?path=$1"
    },
    {
      "source": "/api/receita-por-veiculo",
      "destination": "/api/receita-por-veiculo.mjs"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}