{
  "version": 2,
  "crons": [
    {
      "path": "/api/cron/finalize-locacoes",
      "schedule": "0 5 * * *"
    }
  ],
  "rewrites": [
    {
      "source": "/api/cron/finalize-locacoes",
      "destination": "/api/cron/finalize-locacoes.mjs"
    },
    {
      "source": "/api/locacoes",
      "destination": "/api/locacoes.mjs"
    },
    {
      "source": "/api/locacoes/(.*)",
      "destination": "/api/locacoes.mjs?path=$1"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}